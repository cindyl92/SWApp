@{
    ViewData["Title"] = "Movie List";
}

<h2>Star Wars Movie List</h2>

<ul>
    @foreach (var film in ViewData["filmList"] as IList<StarWarsApiCSharp.Film>)
    {
        <li>
            <a href='Films/Details?filmId=@film.Url.Substring(27)'>@film.Title (@film.ReleaseDate.Substring(0,4))</a>
        </li>
    }
</ul>
